 /* Remove the navbar's default margin-bottom and rounded borders */ 
    #logo{
    width:220px;
    height:80px;
    }
    .navbar {
      margin-bottom: 0;
      border-radius: 0;
    }
    
   #panellogo{
    color:#fff;
     height:20px;
     width:20px;
    }
    
    #panelbtn{
    font-size:10px;
    background-color:rgb(0, 133, 202);
    }
    #panelsearch{
    width:220px;
    background-color:#fff;
    }
    .panel-heading{
    background-color:#004c75; 
    color:#fff;
    font-size:11px; 
    font-family:arial;
    width:220px;
    height:23px;
    padding-top:5px;
    padding-left:0px;
    padding-bottom:5px;
    }
    
    /* Set height of the grid so .sidenav can be 100% (adjust as needed) */
    .row.content{background-color: #313942;height:100%;}
    
    /* Set gray background color and 100% height */
    .sidenav {
      padding-left: 50px;
	  padding-top: 10px;
    
   height:100%;
     
