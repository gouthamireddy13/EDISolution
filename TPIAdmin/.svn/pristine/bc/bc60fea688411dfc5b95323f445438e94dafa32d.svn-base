			$scope.savetpp = function(contactflag,transactionflag,delimiterflag,flag){
					
			if(!contactflag&&$scope.contactList.length==0)
				{
				$scope.ErrorMsg=true;
				return false;
				}
				if(!transactionflag&&$scope.transactionList.length==0)
				{
				$scope.ErrorMsg1=true;
				return false;
				}
				if(!delimiterflag)
				{
				$scope.ErrorMsgD=true;
				return false;
				}
				
				if(!flag&&$scope.tppType=="1")
				{
				$scope.ErrorMsg2=true;
				return false;
				}
				
				
				$scope.protocol=[];
				var tempproto=[];
				 angular.forEach($scope.ptcolList, function (item) {
					 var obj={};
					 if(item.checked)
						 {
						 obj.id=item.id;
						 tempproto.push(obj);
						 $scope.protoval=false;
						 }
					
				 });
				 
				 if($scope.protoval)
				 {
					 $scope.ErrorMsgP=true;
					 return false;
				 }
				 
				 $scope.protocol=angular.copy(tempproto);
				 var typeobj=
					 {
						id:$scope.tppType 
					 };
				 
					if($scope.tppType=="2" ||$scope.tppType=="3" )	{		
						$scope.lightWellPartner= null;
					};
					var tempList=[];
					$scope.disablesave=true;
					 angular.forEach($scope.backtransactionList, function (item) {
						
							
						 var dir={
								 id:item.direction.id
								 
						 };
						 var doc=
							 {
								 id: item.document.id
							 };
						 var versn=
							 {
								id: item.version.id
							 };
						 var obj1=
							 {
								id:item.id,
								 direction:dir,
								 document:doc,
								 version:versn
							 }
						 tempList.push(obj1);
						 
						 
					 });
						  
					
					 if($scope.repeatDelimiter != undefined || $scope.repeatDelimiter!= null){
					 var submitobj= 
					 {
					     id:$scope.id,
						 type:typeobj,
					     name:$scope.tppName,
						 contactDetails:$scope.contactList,
						 protocols:$scope.protocol,
						 transactions:tempList,
						 segmentDelimiter:{id:$scope.segmentDelimiter},
							 elementDelimiter:{id:$scope.elementDelimiter},
								 compositeElementDelimiter:{id:$scope.compositeElementDelimiter},
									
								 repeatDelimiter:{id:$scope.repeatDelimiter} ,
						 lightWellPartner:$scope.lightWellPartner
						    
					 };
				 }
					 
					 else{
						 var submitobj= 
						 {
						     id:$scope.id,
							 type:typeobj,
						     name:$scope.tppName,
							 contactDetails:$scope.contactList,
							 protocols:$scope.protocol,
							 transactions:tempList,
							 segmentDelimiter:{id:$scope.segmentDelimiter},
								 elementDelimiter:{id:$scope.elementDelimiter},
									 compositeElementDelimiter:{id:$scope.compositeElementDelimiter},
										
									
							 lightWellPartner:$scope.lightWellPartner
							    
						 };
						 
					 }
				 $http.post("/TPIAdmin/app/tpps",submitobj)
					.success(function (response, status, headers, config) {
						 //$scope.grouplist = angular.copy(response);
						// console.log(JSON.stringify(response));
						 $scope.disablesave=false;
						$scope.ErrorMsg=false;
						$scope.ErrorMsgD = false;
						$scope.ErrorMsg1=false;
						 $scope.id=angular.copy(response.id);
						 $scope.contactList=angular.copy(response.contactDetails);
					 })
