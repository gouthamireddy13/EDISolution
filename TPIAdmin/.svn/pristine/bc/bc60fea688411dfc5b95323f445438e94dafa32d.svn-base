					         "<br />status: " + status ;
					     //    "<br />headers: " + jsonFilter(header) +
					       //  "<br />config: " + jsonFilter(config);
					 })
					.then(function(response) {
						$scope.ErrorMsg=false;
						$scope.validId=false;
					    $scope.myWelcome = response;
					    $scope.clickModal();
					    
					});
									
			};
			
			
						
						
