  <script type="text/ng-template"  id="tree_item_renderer.html">
   <span ng-show="data.show||data.layer=='parent'" >

	<input type="checkBox"  ng-show="data.layer!='parent'"  ng-model="data.select" /> 

		{{data.layer}} <b> Name:</b> {{data.name}} <b> SRID:</b> {{data.srId}} 
		<span ng-show = "!$scope.flag">
		<button ng-click="Show(data,data.layer)">+</button> </span>
		<span ng-show = "$scope.flag">
		<button ng-click="Show(data,data.layer)">-</button> </span>
	
	<ul>
        <li ng-repeat="data in data.nodes" ng-include="'tree_item_renderer.html'"></li>
    </ul>
   </span>
    
    
    
    
</script>
